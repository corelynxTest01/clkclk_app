{
  "expo": {
    "name": "clkclk",
    "slug": "clkclk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myClkClkapp",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": false,
    "experiments": {
      "typedRoutes": true,
      "turboModules": false
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/clkclk-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.corelynxtest.clkclk"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/clkclk-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "320b7440-0d63-4e5a-a696-07e7ed45c505"
      }
    }
  }
}
